<% if logged_in? %>
<% if flash[:notice] %>
   <div class="notice"><%= flash[:notice] %></div>
 <% end %>
<h1><%= accounts_header(@user) %></h1>
  <% @user.accounts.each do |b| %>
    <ul><%= render "account", account: b %></ul>

  <% end %>
  <p><%= link_to "Add New Account", new_user_account_path(current_user)%></p>
<%= link_to "See All User Transactions", account_transactions_path(current_user) %>
  <% else %>
  <h1>You must sign in to continue.</h1>
  <%= link_to "Signin", signin_path %><br>
<% end %>
